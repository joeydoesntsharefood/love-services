generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Confess {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  messages Json
  email String
  question String
  buttons Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}